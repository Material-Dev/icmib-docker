{
  "name": "Feedback",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "feedback",
  "workspaceFolder": "/feedback",
  "shutdownAction":"none",
  "containerEnv": {
    "USER": "${localEnv:USER}"
  },
"runArgs": ["--init"],
"mounts": [ "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind" ],
  "customizations": {
		"vscode": {
			"extensions": [
        "eamodio.gitlens",
        "ms-azuretools.vscode-docker",
        "JakeBecker.elixir-ls",
        "Gruntfuggly.todo-tree",
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ]
		}
	}
}
