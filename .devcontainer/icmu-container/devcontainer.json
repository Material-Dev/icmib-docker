{
  "name": "ICMU",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "icmu",
  "workspaceFolder": "/icmu",
  "shutdownAction":"none",
  "containerEnv": {
    "USER": "${localEnv:USER}"
  },
"runArgs": ["--init"],
"mounts": [ "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind" ],
  "customizations": {
		"vscode": {
			"extensions": [
        "eamodio.gitlens",
        "ms-azuretools.vscode-docker"
      ]
		}
	}
}
